TOKEN=<"bot_token">
CLIENT_ID=<"client_id">
MYSQL_HOST=<"db_address">
MYSQL_PORT=<"db_port">
MYSQL_USER=<"db_user">
MYSQL_PASSWORD=<"db_passwd">
MYSQL_DB=<"db_name">
YOUTUBE_API_KEY=<"yt_api_v3_key">
LOG_DIR=logs
LOG_MAX_SIZE_MB=5
LOG_MAX_FILES=5

# Web API Configuration
API_PORT=3001
ENABLE_API=true
CORS_ORIGIN=https://your-domain.com
SESSION_SECRET=<"random_secret_string">
DISCORD_CLIENT_SECRET=<"discord_oauth_client_secret">
OAUTH_REDIRECT_URI=https://your-domain.com/api/auth/callback
FRONTEND_URL=https://your-domain.com

# Music playback (optional)
# Set true to resolve direct stream URLs during search (slower, legacy behavior).
YTDLP_RESOLVE_DIRECT_URL=false
# Search provider for YouTube queries: auto | api | ytdlp
YT_SEARCH_PROVIDER=auto
# Use a faster direct URL fetch (no JSON/headers). Falls back to full JSON if needed.
YTDLP_DIRECT_FAST=true
# Timeout for yt-dlp direct URL fetch (ms)
YTDLP_DIRECT_TIMEOUT_MS=15000
# How long to wait for a direct URL before starting playback (ms). Blank = wait fully.
YTDLP_DIRECT_WAIT_MS=
# yt-dlp tuning (optional)
YTDLP_FORCE_IPV4=false
# Example: youtube:player_client=android
YTDLP_EXTRACTOR_ARGS=
